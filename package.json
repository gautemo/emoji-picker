{
  "name": "EmojisPicker",
  "version": "1.0.0",
  "description": "Application for finding and copying emoji's",
  "main": "main.js",
  "scripts": {
    "dev": "npm run dev --prefix app",
    "start": "npm run build --prefix app && electron .",
    "build": "npm run build --prefix app && electron-packager . --icon icon.ico --out ./dist --overwrite --ignore=\"scraper|app|microsoft-store\"",
    "update-emojis": "npm run scrape --prefix scraper",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gautemo/emoji-picker.git"
  },
  "author": "Gaute Meek Olsen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gautemo/emoji-picker/issues"
  },
  "homepage": "https://github.com/gautemo/emoji-picker#readme",
  "devDependencies": {
    "electron": "^9.3.1",
    "electron-packager": "^15.1.0"
  }
}
